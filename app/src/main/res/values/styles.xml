<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Header">
        <item name="fontFamily">@font/open_sans_bold</item>
        <item name="android:textSize">@dimen/main_head_font_size</item>
    </style>

    <style name="high_emphasis">
        <item name="fontFamily">@font/open_sans_semibold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="alpha">0.87</item>
    </style>

    <style name="font_button">
        <item name="fontFamily">@font/open_sans_semibold</item>
        <item name="android:textSize">14sp</item>
        <item name="textAllCaps">true</item>
        <item name="android:textColor">@color/white</item>

    </style>

</resources>